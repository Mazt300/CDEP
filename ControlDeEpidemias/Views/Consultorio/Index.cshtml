@model IEnumerable<ControlDeEpidemias.Models.Consultorio>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nombreconsultorio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.estado)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombreconsultorio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.estado)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idconsultorio }) |
            @Html.ActionLink("Details", "Details", new { id=item.idconsultorio }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idconsultorio })
        </td>
    </tr>
}

</table>
